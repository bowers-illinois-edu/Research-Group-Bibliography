@article{Imbens2005,
abstract = { An instrument or instrumental variable manipulates a treatment and$\backslash$naffects the outcome only indirectly through its manipulation of the$\backslash$ntreatment. For instance, encouragement to exercise might increase$\backslash$ncardiovascular fitness, but only indirectly to the extent that it$\backslash$nincreases exercise. If instrument levels are randomly assigned to$\backslash$nindividuals, then the instrument may permit consistent estimation$\backslash$nof the effects caused by the treatment, even though the treatment$\backslash$nassignment itself is far from random. For instance, one can conduct$\backslash$na randomized experiment assigning some subjects to 'encouragement$\backslash$nto exercise' and others to 'no encouragement' but, for reasons of$\backslash$nhabit or taste, some subjects will not exercise when encouraged and$\backslash$nothers will exercise without encouragement; none-the-less, such an$\backslash$ninstrument aids in estimating the effect of exercise. Instruments$\backslash$nthat are weak, i.e. instruments that have only a slight effect on$\backslash$nthe treatment, present inferential problems. We evaluate a recent$\backslash$nproposal for permutation inference with an instrumental variable$\backslash$nin four ways: using Angrist and Krueger's data on the effects of$\backslash$neducation on earnings using quarter of birth as an instrument, following$\backslash$nBound, Jaeger and Baker in using simulated independent observations$\backslash$nin place of the instrument in Angrist and Krueger's data, using entirely$\backslash$nsimulated data in which correct answers are known and finally using$\backslash$nstatistical theory to show that \&quot;only\&quot; permutation inferences$\backslash$nmaintain correct coverage rates. The permutation inferences perform$\backslash$nwell in both easy and hard cases, with weak instruments, as well$\backslash$nas with long-tailed responses. Copyright 2005 Royal Statistical Society.},
author = {Imbens, Guido W. and Rosenbaum, Paul R.},
doi = {10.1111/j.1467-985X.2004.00339.x},
issn = {09641998},
journal = {Journal of the Royal Statistical Society. Series A: Statistics in Society},
keywords = {Hodges-Lehmann estimate,Instrumental variable,Observational study,Permutation test,Randomization test},
pages = {109--126},
title = {{Robust, accurate confidence intervals with a weak instrument: Quarter of birth and education}},
volume = {168},
year = {2005}
}
