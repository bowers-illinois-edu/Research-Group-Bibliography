howpublished = {Paperback},
isbn = {1412903629},
keywords = {lfm,place},
month = mar,
publisher = {Sage Publications Ltd},
title = {{For Space}},
url = {http://www.amazon.ca/exec/obidos/redirect?tag=citeulike09-20\&amp;path=ASIN/1412903629},
year = {2005}
}
