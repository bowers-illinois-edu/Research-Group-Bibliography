@misc{Rosenbaum2007a,
abstract = {In observational studies of treatment effects, matched samples have$\backslash$ntraditionally been constructed using two tools, namely close matches$\backslash$non one or two key covariates and close matches on the propensity$\backslash$nscore to stochastically balance large numbers of covariates. Here$\backslash$nwe propose a third tool, fine balance, obtained using the assignment$\backslash$nalgorithm in a new way. We use all three tools to construct a matched$\backslash$nsample for an ongoing study of provider specialty in the treatment$\backslash$nof ovarian cancer. Fine balance refers to exact balance of a nominal$\backslash$ncovariate, often one with many categories, but it does not require$\backslash$nindividually matched treated and control subjects for this variable.$\backslash$nIn the example the nominal variable has 72 = 9 x 8 categories formed$\backslash$nfrom 9 possible years of diagnosis and 8 geographic locations or$\backslash$nSEER sites. We obtain exact balance on the 72 categories and close$\backslash$nindividual matches on clinical stage, grade, year of diagnosis, and$\backslash$nother variables using a distance, and stochastically balance a total$\backslash$nof 61 covariates using a propensity score. Our approach finds an$\backslash$noptimal match that minimizes a suitable distance subject to the constraint$\backslash$nthat fine balance is achieved. This is done by defining a special$\backslash$npatterned distance matrix and passing it to a subroutine that solves$\backslash$nthe optimal assignment problem, which optimally pairs the rows and$\backslash$ncolumns of a matrix using a polynomial time algorithm. In the example$\backslash$nwe used the function Proc Assign in SAS. A new theorem shows that$\backslash$nwith our patterned distance matrix, the assignment algorithm returns$\backslash$nan optimal, finely balanced matched sample whenever one exists, and$\backslash$notherwise returns an infinite distance, indicating that no such matched$\backslash$nsample exists.},
author = {Rosenbaum, Paul R and Ross, Richard N and Silber, Jeffrey H},
booktitle = {Journal of the American Statistical Association},
doi = {10.1198/016214506000001059},
issn = {0162-1459},
pages = {75--83},
title = {{Minimum Distance Matched Sampling With Fine Balance in an Observational Study of Treatment for Ovarian Cancer}},
volume = {102},
year = {2007}
}
