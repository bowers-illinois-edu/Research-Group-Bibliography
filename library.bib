@misc{Rosenbaum2008,
abstract = {Often, the protocol for a randomized controlled trial states that$\backslash$nif a patient's health deteriorates substantially in certain specific$\backslash$nways, then the study treatments will no longer be considered adequate$\backslash$nmedical care, and the patient will be taken off the study treatments$\backslash$nand given the needed care. Aberrant responses of this sort present$\backslash$nspecial problems of interpretation. The few such patients may have$\backslash$nreceived only limited exposure to the study treatments, but also$\backslash$nmay have experienced some of the worst outcomes observed in the trial.$\backslash$nThe most basic question is whether one of the treatments tends to$\backslash$ncause such aberrant responses, quite apart from any other beneficial$\backslash$nor harmful effects that treatment may have. Common practice is to$\backslash$nreduce aberrant responses to binary events and apply Fisher's exact$\backslash$ntest for a 2 x 2 table, but this approach discards the available$\backslash$ninformation about the magnitude of the aberration. We propose a new$\backslash$nrandomization test for this problem that uses information about both$\backslash$nthe number of aberrations and their magnitude. In extreme situations,$\backslash$nthe new test reduces to either Fisher's exact test or Wilcoxon's$\backslash$nrank sum test, but in common situations, the new test has features$\backslash$nof both methods. Importantly, this exact test reacts only to aberrant$\backslash$neffects; it is completely unaffected by treatment effects that do$\backslash$nnot produce aberrations. When aberrant responses have several aspects,$\backslash$nwe examine the consequences of selecting, after the fact, the one$\backslash$naspect exhibiting the greatest aberration, and applying the test$\backslash$nto that aspect. For a single response variable, a confidence interval$\backslash$nis proposed for an additive aberrant effect; the interval is constructed$\backslash$nby inverting the test, and a multivariate extension is briefly mentioned.$\backslash$nWe illustrate the use of the test in the ACE-Inhibitor After Anthracycline$\backslash$n(AAA) randomized trial, which was aimed at preserving cardiac function$\backslash$nin children treated for cancer. Seven of 135 children were removed$\backslash$nfrom the trial due to substantial cardiac declines; 6 of these were$\backslash$non placebo, I of whom subsequently died.},
author = {Rosenbaum, Paul R and Silber, Jeffrey H},
booktitle = {Journal of the American Statistical Association},
doi = {10.1198/016214507000001274},
isbn = {0162145070000},
issn = {0162-1459},
pages = {240--247},
title = {{Aberrant Effects of Treatment}},
volume = {103},
year = {2008}
}
