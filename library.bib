isbn = {1528-1140 (Electronic)$\backslash$r0003-4932 (Linking)},
issn = {0003-4932},
pages = {79--86},
pmid = {22566017},
title = {{Medical and Financial Risks Associated With Surgery in the Elderly Obese}},
