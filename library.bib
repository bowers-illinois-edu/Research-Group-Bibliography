@article{Rosenbaum2001,
abstract = {In randomisation and permutation inference, pivotal arguments remove the hypoth- esised treatment effect, thereby basing inferences on the null distribution in which the treatment has no effect. This is common, for instance, with additive treatment effects. The current paper uses 'attributable effects' to expand substantially the scope of pivotal argu- ments. Attributable effects are defined for three cases, namely the 2 x 2 contingency table, displacement effects and the Mann-Whitney-Wilcoxon statistic, and in each case removing an appropriate attributable effect restores the familiar null randomisation distribution of the associated statistic, yielding exact inferences. The procedure extends immediately for use in sensitivity analysis in nonrandomised observational studies.},
author = {Rosenbaum, Paul R.},
doi = {10.1093/biomet/88.1.219},
issn = {0006-3444},
