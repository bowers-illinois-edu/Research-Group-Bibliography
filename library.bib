publisher = {Blackwell Publishing Inc},
title = {{Civic Engagement and Education: An Empirical Test of the Sorting Model}},
url = {http://dx.doi.org/10.1111/j.1540-5907.2009.00400.x},
volume = {53},
year = {2009}
}
