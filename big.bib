@Article{	  tsui1989gps,
  author	= {Tsui, Kam-Wah and Weerahandi, Samaradas},
  copyright	= {Copyright {\^A}{\copyright} 1989 American Statistical
		  Association},
  date-added	= {2014-03-31 11:39:34 +0000},
  date-modified	= {2016-11-02 03:20:24 +0000},
  issn		= {01621459},
  journal	= {Journal of the American Statistical Association},
  jstor_articletype={primary_article},
  jstor_formatteddate={Jun., 1989},
  number	= {406},
  pages		= {602--607},
  publisher	= {American Statistical Association},
  title		= {Generalized p-Values in Significance Testing of Hypotheses
		  in the Presence of Nuisance Parameters},
  volume	= {84},
  year		= {1989},
  abstract	= {This article examines some problems of significance
		  testing for one-sided hypotheses of the form H0: {\^I}
		  {\^a} {\^I}0 versus $H_1: \theta > \theta_0$ , where
		  {\^I} is the parameter of interest. In the usual setting,
		  let x be the observed data and let T(X) be a test statistic
		  such that the family of distributions of T(X) is
		  stochastically increasing in {\^I}. Define Cx as {X: T(X)
		  - T(x) {\^a} 0}. The p value is $p(x) = \sup_{\theta \leq
		  \theta_0} \Pr(X \in C_x \mid \theta)$ . In the presence of
		  a nuisance parameter {\^I}, there may not exist a
		  nontrivial Cx with a p value independent of {\^I}. We
		  consider tests based on generalized extreme regions of the
		  form Cx({\^I}, {\^I}) = {X: T(X; x, {\^I}, {\^I})
		  {\^a} T(x; x, {\^I}, {\^I})}, and conditions on T(X, x,
		  {\^I}, {\^I}) are given such that the p value $p(x) =
		  \sup_{\theta \leq \theta_0} Pr(X \in C_x \mid \theta)$ is
		  free of the nuisance parameter {\^I}, where T is
		  stochastically increasing in {\^I}. We provide a solution
		  to the problem of testing hypotheses about the differences
		  in means of two independent exponential distributions, a
		  problem for which the fixed-level testing approach has not
		  produced a nontrivial solution except in a special case. We
		  also provide an exact solution to the Behrens-Fisher
		  problem. The p value for the Behrens-Fisher problem turns
		  out to be numerically (but not logically) the same as
		  Jeffreys's Bayesian solution and the Behrens-Fisher
		  fiducial solution. Our approach of testing on the basis of
		  p values is especially useful in multiparameter problems
		  where nontrivial tests with a fixed level of significance
		  are difficult or impossible to obtain.},
  bdsk-url-1	= {http://www.jstor.org/stable/2289949}
}

