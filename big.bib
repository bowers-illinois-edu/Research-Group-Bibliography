@article{gueorguiev2012,
	Abstract = {Among the concerns faced by countries pondering the costs
		  and benefits of greater economic openness to international
		  capital flows is the worry that new and powerful external
		  actors will exert a corrupting influence on the domestic
		  economy. In this paper, we use a novel empirical strategy,
		  drawn from research in experimental psychology, to test the
		  linkage between foreign direct investment (FDI) and
		  corruption. The prevailing literature has produced confused
		  and contradictory results on this vital relationship due to
		  errors in their measurement of corruption which are
		  correlated with FDI inflows. When a less biased
		  operationalization is employed, we find clear evidence of
		  corruption during both registration and procurement
		  procedures in Vietnam. The prevalence of corruption,
		  however, is not associated with inflows of FDI. On the
		  contrary, one measure of economic openness appears to be
		  the most important driver of reductions in Vietnamese
		  corruption: the wave of domestic legislation, which
		  accompanied the country's bilateral trade liberalization
		  agreement with the United States (US-BTA), significantly
		  reduced bribery during business registration. ?? 2011
		  Elsevier Inc.},
	Author = {Gueorguiev, Dimitar and Malesky, Edmund},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Isbn = {1049-0078},
	Issn = {10490078},
	Journal = {Journal of Asian Economics},
	Keywords = {Bribery,Corruption,FDI,List question,Unmatched count technique,Vietnam},
	Pages = {111--129},
	Title = {{Foreign investment and bribery: A firm-level analysis of corruption in Vietnam}},
	Volume = {23},
	Year = {2012},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.asieco.2011.11.006}}

