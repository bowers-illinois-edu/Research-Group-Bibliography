@InCollection{	  wantchekon2011,
  author	= {Wantchekon, Leonard and Vermeersch, Christel},
  booktitle	= {From Inertia to Public Action},
  date-modified	= {2018-06-11 15:13:09 +0000},
  isbn		= {978-0-8213-8556-2},
  keywords	= {ACCESS TO INFORMATION,CANDIDATES,CITIZEN GROUPS,CIVIC
		  ENGAGEMENT,COMMUNITY INVOLVEMENT,DEMOCRACIES,ECONOMIC
		  EFFICIENCY,ECONOMIC GROWTH,ECONOMIC
		  PERFORMANCE,ELECTION,ELECTRICITY,EXTERNALITIES,HOUSING,PRESIDENTIAL
		  ELECTIONS,PUBLIC,PUBLIC ECONOMICS,PUBLIC FUNDS,PUBLIC GOOD
		  APPROACH,PUBLIC GOODS,PUBLIC OPINION,PUBLIC
		  SPENDING,ROADS,TRANSPARENCY,VOTING,VOTING PATTERNS,WOMEN},
  pages		= {123--136},
  title		= {{Information, Social Networks, and the Demand for Public
		  Goods: Experimental Evidence from Benin}},
  year		= {2011},
  abstract	= {Reports on an investigation carried out during the 2001
		  presidential elections in Benin of the extent to which
		  information and civic community affect voters'
		  responsiveness to national public goods platforms. Access
		  to media and membership in local organizations are
		  associated with lower distaste for the public goods
		  platform, whereas persons more involved in political
		  discussions tend to show higher distaste for it. Results
		  suggest that public access to information and use of media
		  outlets may not only make governments more responsive, but
		  also shape voters' demand for growth-promoting policies.
		  Paradoxically, ethnic ties between voters and candidates
		  can increase the demand for national public goods. Ethnic
		  ties and media access have similar effects, arguably
		  because they both enhance voters' ability to monitor
		  politicians. Women, who act as driving forces behind
