@misc{hatemi2012a,
	Abstract = {For the greater part of human history, political
		  behaviors, values, preferences, and institutions have been
		  viewed as socially determined. Discoveries during the 1970s
		  that identified genetic influences on political
		  orientations remained unaddressed. However, over the past
		  decade, an unprecedented amount of scholarship utilizing
		  genetic models to expand the understanding of political
		  traits has emerged. Here, we review the 'genetics of
		  politics', focusing on the topics that have received the
		  most attention: attitudes, ideologies, and pro-social
		  political traits, including voting behavior and
		  participation. The emergence of this research has sparked a
		  broad paradigm shift in the study of political behaviors
		  toward the inclusion of biological influences and
		  recognition of the mutual co-dependence between genes and
		  environment in forming political behaviors. ?? 2012
		  Elsevier Ltd.},
	Author = {Hatemi, Peter K. and McDermott, Rose},
	Booktitle = {Trends in Genetics},
