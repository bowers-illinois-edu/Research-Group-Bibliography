@article{paluck2010b,
	Abstract = {Randomized field experiments should take a more central
		  place in qualitative research. Although field
		  experimentation is often considered a quantitative
		  enterprise, this paper illustrates the compatibility of
		  field experimentation with various types of qualitative
		  measurement tools and research questions. Integrating
		  qualitative and quantitative data within field experiments
		  allows investigators to move past simple average treatment
		  effects and explore mechanisms of the identified causal
		  effect. A more novel proposal is to use field
		  experimentation as the organizing methodological framework
		  for archival, ethnographic, or interpretive work, and to
		  use ethnographic methods as the primary source of
		  measurement in ``experimental ethnography.'' Sustained
		  research and theoretical specificity can address some of
		  the seemingly incompatible features of qualitative and
		  field experimental methods. For example, small sample sizes
		  are acceptable as part of a research program, and some
		  theories of historical patterns or rare events could be
		  disaggregated into smaller cause and effect linkages to
		  test with field experiments in theoretically relevant
		  contexts.},
	Author = {Paluck, Elizabeth Levy},
	Date-Modified = {2018-06-11 15:13:08 +0000},
	Issn = {0002-7162},
	Journal = {The Annals of the American Academy of Political and Social Science},
	Keywords = {archival methods,experimental ethnography,field experimentation,interpretive methods,qualitative methods},
	Pages = {59--71},
	Title = {{The Promising Integration of Qualitative Methods and Field Experiments}},
	Year = {2010},
	Bdsk-Url-1 = {https://doi.org/10.1177/0002716209351510}}

