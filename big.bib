  issn		= {00063444},
  journal	= {Biometrika},
  keywords	= {Detecting bias,Observational study,Permutation
		  inference,Sensitivity analysis},
  pages		= {367--374},
  title		= {{Detecting bias with confidence in observational
		  studies}},
  volume	= {79},
  year		= {1992},
  abstract	= {In an observational study, detecting hidden bias
		  involves$\backslash$nchecking that treatment effects appear
		  where they should,$\backslash$nand not elsewhere. For
		  instance, treated and control groups$\backslash$nare often
		  compared with respect to outcomes the
		  treatment$\backslash$nshould not affect. This paper uses
		  such a test for bias to$\backslash$nobtain a confidence set
		  for an unobserved covariate. The$\backslash$nimpact of this
		  unobserved covariate is indicated by
		  a$\backslash$nsensitivity analysis with the covariate
		  confined to the$\backslash$nconfidence set. In this way, a
		  test for bias may indicate$\backslash$neither the presence
		  and magnitude of a hidden bias or else$\backslash$na
		  reduction in the sensitivity to bias.},
  bdsk-url-1	= {https://doi.org/10.1093/biomet/79.2.367}
}

