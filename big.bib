	Keywords = {political_participation political_psychology political_science social_capital},
	Month = {April},
	Opturl = {http://www.amazon.fr/exec/obidos/ASIN/0674007972/citeulike04-21},
	Publisher = {{Harvard University Press}},
	Title = {Loose Connections: Joining Together in America's Fragmented Communities},
	Year = {2002}}

