@article{loewen2008,
	Abstract = {Public opinion researchers interested in testing the
		  comparative power of arguments are con-$\backslash$nfronted
		  with two problems: statistical power and learning. As the
		  number of arguments$\backslash$nincreases and/or a sample
		  size decreases, the ability of conventional means tests and
		  regres-$\backslash$nsion models to uncover small but real
		  effects declines. We present a solution to this problem
		  of$\backslash$nthe pairwise comparison of arguments: the
		  Bradley-Terry model. Using a survey
		  experiment$\backslash$nconducted during a referendum on
		  electoral reform in the province of Ontario, Canada in
		  the$\backslash$nfall of 2007, we show how unstructured and
		  structured models can uncover information
		  about$\backslash$nthe power of arguments and its sources
		  undetected by more conventional methods. In
		  doing$\backslash$nso, we gain insight into the factors
		  which drive support or opposition for electoral reform.
		  We$\backslash$nconclude by discussing the limits and
		  further applications of these models.},
	Author = {Loewen, Peter John and Rubenson, Daniel and Spirling, Arthur},
	Journal = {Labour},
	Title = {{Testing the Power of Arguments with a Bradley-Terry Model : An Application to Electoral Reform ∗}},
	Year = {2008}}

@misc{loewen2008a,
	Abstract = {Does compulsory voting lead to more knowledgeable and
		  engaged citizens? We report the results from a recent
		  experiment measuring such ``second-order effects'' in a
