@article{gastwirth1998,
	Abstract = {When a study shows an association between a treatment and
		  a response,$\backslash$nbefore concluding that there is a
		  causal relationship it is useful$\backslash$nto assess
		  whether or not an unobserved variable, U,might
		  explain$\backslash$nthe observed association. Sensitivity
		  analysis clarifies the properties$\backslash$nU must have
		  in terms of its relationship to the response and
		  its$\backslash$nimbalance in the groups being compared. A
		  substantial literature$\backslash$nhas investigated the
		  imbalance or association between U and
		  group$\backslash$nassignment. This paper develops a dual
		  sensitivity analysis for matched$\backslash$npairs,
		  focusing on the strength of the relationship between U
		  and$\backslash$nthe response required to reduce an observed
		  association to non-significance.$\backslash$nA third
		  simultaneous form of sensitivity analysis models both
		  relationships$\backslash$nbetween U and treatment
		  assignment and U and response. The
		  simultaneous$\backslash$nform allows one to compare results
		  from the sensitivity analysis$\backslash$nto subject matter
		  knowledge about both relationships. The
		  methods$\backslash$nare illustrated by several examples.},
	Author = {Gastwirth, Joseph L and Krieger, Abba M and Rosenbaum, Paul R and Washington, George},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Issn = {00063444},
	Journal = {Biometrika},
	Pages = {907--920},
	Title = {{Dual and simultaneous sensitivity analysis for matched pairs}},
	Volume = {85},
	Year = {1998},
	Bdsk-Url-1 = {http://biomet.oxfordjournals.org/content/85/4/907.short}}

@article{gastwirth2007,
	Abstract = {Although it is often true that the association between two
		  variables may be due to some unobserved third variable, the
		  plausiblity of such arguments needs careful examination. In
		  1987, a Canadian court accepted an explanation offered by
		  the National Revenue Service of Canada that, in a test for
		  promotion, lower pass rates among women than among men were
		  explained by differences in rates of college attendance, a
		  variable that was not directly observed by the court. We
		  show that this explanation is not merely implausible. It is
		  impossible. We conclude with a brief discussion of the role
		  of a statistician in an argument involving an unobserved
		  variable.},
	Author = {Gastwirth, Joseph and Krieger, Abba and Rosenbaum, Paul},
