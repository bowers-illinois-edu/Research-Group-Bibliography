@misc{jensen2005,
	Abstract = {Political risk is an important factor in the decision to
		  invest abroad.$\backslash$nWhile the investment potential
		  might be lucrative, there is always$\backslash$nthe risk
		  that the host government will expropriate the profits
		  and$\backslash$nassets of the foreign investor. Political
		  institutions, however,$\backslash$ncan serve as constraints
		  on the actions of political actors in the$\backslash$nhost
		  country. We argue that federal structures lower political
		  risk.$\backslash$nJoint-reputational accountability in
		  overlapping political jurisdictions$\backslash$nincreases
		  the likelihood that investment contracts will be
		  honored.$\backslash$nEmpirical analyses of cross-sectional
		  time-series data for 115 countries,$\backslash$nfrom
		  1975-1995, are used to study how political institutions
		  affect$\backslash$nforeign direct investment (FDI) flows.
		  After controlling for the$\backslash$neffect of relevant
		  economic and political variables, we find
		  that$\backslash$nboth democratic and federal institutions
		  help attract FDI, although$\backslash$nthe additive effect
		  of democracy and federalism is small. This
		  is$\backslash$nnot surprising; democratic systems already
		  have low political risk;$\backslash$nthey do not need the
		  additional credibility that the federal
		  system$\backslash$nprovides to attract FDI. In contrast, we
		  expect that federal structures$\backslash$nsignificantly
		  improve the trustworthiness of less democratic
		  states.$\backslash$nEmpirically, we find that less
		  democratic countries with federal$\backslash$npolitical
		  systems attract some of the highest levels of FDI.},
	Author = {Jensen, Nathan and McGillivray, Fiona},
