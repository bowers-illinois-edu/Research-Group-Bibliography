	Language = {English},
	Number = {2},
	Pages = {pp. 333-351},
	Publisher = {Institute of Mathematical Statistics},
	Title = {Ancillaries and Conditional Inference},
	Url = {http://www.jstor.org/stable/4144416},
	Volume = {19},
	Year = {2004},
