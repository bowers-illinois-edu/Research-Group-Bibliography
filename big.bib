	Pages = {487--508},
	Title = {{Opiates for the Matches: Matching Methods for Causal Inference}},
	Volume = {12},
	Year = {2009},
	Bdsk-Url-1 = {https://doi.org/10.1146/annurev.polisci.11.060606.135444}}

