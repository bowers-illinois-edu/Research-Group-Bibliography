  date-modified	= {2018-06-11 15:13:08 +0000},
  issn		= {02779536},
  journal	= {Social Science and Medicine},
  keywords	= {Domestic violence,Dowry,Wife abuse},
  pages		= {1169--1179},
  pmid		= {9131741},
  title		= {{Wife-beating in rural South India: A qualitative and
		  econometric analysis}},
  volume	= {44},
  year		= {1997},
  abstract	= {This paper uses ethnographic and econometric methods to
		  study the determinants of wife abuse in a community of
		  potters in Southern India. The qualitative work
		  demonstrates that wife abuse is more likely when dowries
		  are perceived as inadequate, when husbands are alcoholic,
		  and when the cause of the abuse is perceived as
		  'legitimate' by the community. Hypotheses generated from
		  the qualitative work are tested with survey data, which
		  confirm that the risk of wife abuse significantly increases
		  with smaller dowry payments and alcohol consumption.
		  Furthermore, the statistical analysis reveals that women
		  who are sterilized or have fewer male children are more
		  likely to be abused.},
  bdsk-url-1	= {https://doi.org/10.1016/S0277-9536(96)00252-3}
}

@InCollection{	  rao2003,
  author	= {Rao, Vijayendra and Woolcock, Michael},
  booktitle	= {\ldots : Evaluation techniques and tools},
  date-modified	= {2018-06-11 15:13:08 +0000},
  pages		= {165--190},
  title		= {{Integrating qualitative and quantitative approaches in
		  program evaluation}},
  year		= {2003},
  abstract	= {This chapter outlines some of the ways and means by which
		  inte- grating qualitative and quantitative approaches in
		  development research and program evaluation can help yield
		  insights that neither approach would produce on its own. In
		  assessing the impact of development programs and policies,
		  it is important to recognize that the quantitative methods
		  emphasized in this tool kit, while enormously useful,
		  nonetheless have some important limitations and that some
		  of these can be overcome by incorporating comple- mentary
		  qualitative approaches. An examination of the strengths and
		  weakness of orthodox stand-alone quantitative (and qualita-
		  tive) approaches is followed by a basic framework for
		  integrating different approaches, based on distinguishing
		  between data and the methods used to collect them. Some
		  practical examples of "mixed- method" approaches to program
		  evaluation are then given, and some conclusions drawn.},
  bdsk-url-1	= {http://siteresources.worldbank.org/INTPSIA/Resources/490023-1121114603600/12930%5C_Chapter8.pdf}
}

@Misc{		  rao2005,
  author	= {Rao, Vijayendra and Ib\'{a}\~{n}ez, Ana Mar\'{\i}a},
  booktitle	= {Journal of Development Studies},
  date-modified	= {2018-06-11 15:13:08 +0000},
  isbn		= {0022-0388},
  issn		= {0022-0388},
  pages		= {788--838},
  title		= {{The Social Impact of Social Funds in Jamaica: A
		  `Participatory Econometric' Analysis of Targeting,
		  Collective Action, and Participation in Community-Driven
		  Development}},
  volume	= {41},
  year		= {2005},
  abstract	= {Qualitative data from a case study of the Jamaica social
		  investment fund reveal that the social fund process is
		  elite-driven and decision-making tends to be dominated by a
		  small group of motivated individuals. However, there is
		  broad-based satisfaction with the outcome. Quantitative
		  data from 500 households mirror these findings by showing
		  that, ex-ante, the social fund does not address the
		  expressed needs of the majority of individuals in the
		  majority of communities. By the completion of the project,
		  however, 80 per cent of the community expresses
		  satisfaction with the outcome. An analysis of the
		  determinants of participation reveals that better educated
		  and better networked individuals dominate the process.
		  Propensity-score analysis demonstrates that JSIF has had a
		  causal impact on improvements in trust and the capacity for
		  collective action, but these gains are greater for
		  elites.},
  bdsk-url-1	= {https://doi.org/10.1080/00220380500145297}
}

@Article{	  rao2008,
  author	= {Rao, Vijayendra},
  journal	= {The Contested Commons: Conversations Between Economists
		  and Anthropologists},
  pages		= {1--26},
  title		= {{Symbolic Public Goods and the Coordination of Collective
		  Action: A Comparison of Local Development in India and
		  Indonesia}},
  year		= {2008},
  abstract	= {Most economists think of common property as physical -- a
		  body of water, a forest -- and as $\backslash$nbounded
		  within geographic space. In this paper, building on work in
		  social theory, I argue that $\backslash$ncommon property
		  can also be social -- defined within symbolic space. People
		  can be bound by $\backslash$nwell-defined symbolic
		  agglomerations that have characteristics similar to common
		  property. I $\backslash$ncall these ``symbolic public
		  goods'' (SPGs) and make the case that such constructs are
		  central to $\backslash$nunderstanding collective action.
		  The point is illustrated by contrasting how conceptions of
		  $\backslash$nnationalism in Indonesia and India created
		  SPGs that resulted in very different strategies of local
		  $\backslash$ndevelopment. Indonesia emphasized collective
		  action by the poor that resulted in a form of
		  $\backslash$nregressive taxation, enforced by the ideology
		  of svadaya gotong royong (community self-help)
		  $\backslash$nthat was both internalized and coercively
		  enforced. India emphasized democratic decentralization
		  $\backslash$nvia the panchayat system driven by the
		  Gandhian ideology of gram swaraj (self-reliant villages).
		  $\backslash$nThis has resulted in an unusual
		  equity-efficiency tradeoff; Indonesia has delivered public
		  services $\backslash$nmore efficiently than India, but at
		  the cost of democratic freedoms and voice. I argue that the
		  $\backslash$nchallenge for these countries is to not
		  undermine their existing SPGs but to build on them;
		  $\backslash$nIndonesia should retain the spirit of svadaya
		  gotong royong but to channel it in an equitable and
		  $\backslash$ndemocratic direction, while India should build
		  the capacity of the panchayat system by giving it
		  $\backslash$nfiscal teeth, while promoting underutilized
		  institutions such as Gram Sabhas that encourage
		  $\backslash$naccountability and transparency.}
}

