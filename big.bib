@Misc{		  shocket1992,
  author	= {Shocket, P. A. and Heighberger, N. R. and Brown, C.},
  booktitle	= {Political Research Quarterly},
  date-modified	= {2018-06-11 15:13:08 +0000},
  isbn		= {0043-4078},
  issn		= {1065-9129},
  keywords	= {elec\_admin},
  mendeley-tags	= {elec\_admin},
  pages		= {521--537},
  title		= {{The Effect of Voting Technology On Voting Behavior in a
		  Simulated Multi-Candidate City Council Election: a
		  Political Experiment of Ballot Transparency}},
  volume	= {45},
  year		= {1992},
  abstract	= {The relationship between voting behavior and ballot design
		  and construction has been largely ignored. A political
		  experiment was conducted to test the impact of three voting
		  technologies (manual paper and pencil, computerized
		  punch-card, and direct recording electronic) on voting
		  behavior operationalized as the number of votes cast in a
		  simulated multi-candidate field election. In a controlled
		  experiment, subjects were shown political commercials for
		  eighteen candidates, asked to choose up to nine candidates,
		  and then randomly assigned to one of the three voting
		  systems. The analyses confirm three hypotheses at the
		  individual level which have been suggested previously by
		  aggregate level analysis. Compared to the other voting
		  systems, subjects using the computerized punch-card system
		  less frequently exercised their full franchise by voting
		  for an complete slate of candidates, more frequently
		  spoiled ballots by overvoting, and voted for fewer
		  candidates on average.},
  bdsk-url-1	= {https://doi.org/10.1177/106591299204500213}
}

