@Unpublished{	  glynn2013a,
  author	= {Glynn, Adam N and Ichino, Nahomi},
  date-modified	= {2018-06-11 15:13:07 +0000},
  keywords	= {Causal Inference,EGAP},
  mendeley-tags	= {Causal Inference,EGAP},
  title		= {{Using Case Studies to Improve Causal Inference}},
  year		= {2013},
  abstract	= {Using the Rosenbaum (2002, 2009) approach to observational
		  studies, we show how qualita- tive information can be
		  incorporated into quantitative analyses to improve causal
		  inference in three ways. First, we can ameliorate the e
		  ects of dicult-to-measure outcomes by including qualitative
		  information on outcomes within matched sets, sometimes
		  reducing p -values. Second, additional information across
		  matched sets enables the construction of qualitative con
		  dence in- tervals on e ect size. Third, qualitative
		  information on unmeasured confounders within matched sets
		  reduces the conservativeness of Rosenbaum-style sensitivity
		  analysis. This approach accom- modates small to medium
		  sample sizes in a nonparametric framework, and therefore
		  may be particularly useful for analyses of the e ects of
		  institutions in a given set of units. We illus- trate these
		  methods by examining the e ect of using plurality rules in
		  transitional presidential elections on opposition
		  harassment in 1990s sub-Saharan Africa.},
  bdsk-url-1	= {http://polmeth.wustl.edu/media/Paper/GlynnIchinopolmeth20120717.pdf}
}

@Article{	  glynn2014,
  author	= {Glynn, Adam. N. and Ichino, Nahomi},
  date-modified	= {2018-06-11 15:13:07 +0000},
  issn		= {0049-1241},
  journal	= {Sociological Methods \& Research},
  title		= {{Increasing Inferential Leverage in the Comparative
		  Method: Placebo Tests in Small-n Research}},
  year		= {2014},
  abstract	= {We delineate the underlying homogeneity assumption,
		  procedural variants, and implications of the comparative
		  method and distinguish this from Mill's method of
		  difference. We demonstrate that additional units can
		  provide ``placebo'' tests for the comparative method even
		  if the scope of inference is limited to the two units under
		  comparison. Moreover, such tests may be available even when
		  these units are the most similar pair of units on the
		  control variables with differing values of the independent
		  variable. Small-n analyses using this method should
		  therefore, at a minimum, clearly define the dependent,
		  independent, and control variables so they may be measured
		  for additional units, and specify how the control variables
		  are weighted in defining similarity between units. When
		  these tasks are too difficult, process tracing of a single
		  unit may be a more appropriate method. We illustrate these
		  points with applications to two studies. Keywords},
  bdsk-url-1	= {http://smr.sagepub.com/cgi/doi/10.1177/0049124114528879},
  bdsk-url-2	= {https://doi.org/10.1177/0049124114528879}
}

