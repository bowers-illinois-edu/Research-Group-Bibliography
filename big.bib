	Journal = {Journal of Economic Behavior and Organization},
	Keywords = {Dictator game,Fairness,Gender},
	Pages = {143--158},
	Title = {{The relative price of fairness: Gender differences in a punishment game}},
	Volume = {30},
	Year = {1996},
	Bdsk-Url-1 = {https://doi.org/10.1016/S0167-2681(96)00854-2}}

@article{eckel1996a,
	Abstract = {We conduct double-anonymous dictator experiments to
		  explore the role of altruism in motivating subjects'
		  behavior. We vary the extent to which an anonymous
		  recipient is deserving of aid and investigate its effect on
		  the allocation of a fixed pie by student subjects. This is
		  accomplished by including as treatments: (1) an anonymous
		  student subject and (2) an established charity. We find
		  that a significant increase in donations occurs when we
		  increase the extent to which a donation goes to a recipient
		  generally agreed to be ``deserving.'' We conclude that
		  subjects are rational in the way they incorporate fairness
		  into their decisions.Journal of Economic
		  LiteratureClassification Numbers: A13, C91, D64.},
	Author = {Eckel, Catherine C. and Grossman, Philip J.},
