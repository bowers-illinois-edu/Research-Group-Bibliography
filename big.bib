@Misc{		  nickerson2006,
  author	= {Nickerson, David W.},
  booktitle	= {Journal of Political Marketing},
  date-modified	= {2018-06-11 15:13:08 +0000},
  issn		= {1537-7857},
  pages		= {47--69},
  title		= {{Hunting the Elusive Young Voter}},
  volume	= {5},
  year		= {2006},
  abstract	= {It is generally assumed that mobilizing young voters is
		  infeasible. This belief can be broken into two separate
		  questions: (1) Are young people responsive to Get Out The
		  Vote appeals? (2) Is it possible to deliver the message to
		  the intended audience? By pooling together the results of
		  six GOTV field experiments, this paper demonstrates that
		  young voters are equally responsive to blandishments to
		  vote as mature voters, but that they are roughly three
		  times more difficult to contact.},
  bdsk-url-1	= {https://doi.org/10.1300/J199v05n03%5C_03}
}

@InProceedings{	  nickerson2006a,
  author	= {Nickerson, David W},
  booktitle	= {Annual Meeting of the American Political Science
		  Association},
  pages		= {22},
  title		= {{Forget Me Not? The Importance of Timing in Voter
		  Mobilization}},
  year		= {2006},
  abstract	= {Recent field experiments have demonstrated the utility of
		  personalized voter mobilization tactics in a broad range of
		  settings (e.g., Gerber and Green 2000; Green, Gerber, and
		  Nickerson 2003; Michelson 2003; Arceneaux 2005; Nickerson
		  2006a; Nickerson, Friedrichs, and King 2006). While
		  personalized tactics are efficient with regards to cost,
		  coordinating large-scale door-to-door canvassing operations
		  for state-wide offices is a daunting task. Given the
		  hundreds of thousands of voters to be contacted, reaching
		  every targeted person in the last days leading up to
		  Election Day may prove logistically difficult. Expanding
		  the days on which Get Out the Vote (GOTV) activities take
		  place would make conducting labor intensive canvassing
		  campaigns much easier to administer. However, there has
		  been very little attention paid to the timing of campaign
		  outreach and when it is maximally effective. This paper
		  addresses this lacuna by analyzing a door-to-door voter
		  mobilization experiment that randomly varied the timing of
		  campaign outreach.}
}

@InProceedings{	  nickerson2006b,
  author	= {Nickerson, David W},
  booktitle	= {Annual Meeting of the American Political Science
		  Association},
  pages		= {22},
  title		= {{Forget Me Not? The Importance of Timing in Voter
		  Mobilization}},
  year		= {2006},
  abstract	= {Recent field experiments have demonstrated the utility of
		  personalized voter mobilization tactics in a broad range of
		  settings (e.g., Gerber and Green 2000; Green, Gerber, and
		  Nickerson 2003; Michelson 2003; Arceneaux 2005; Nickerson
		  2006a; Nickerson, Friedrichs, and King 2006). While
		  personalized tactics are efficient with regards to cost,
		  coordinating large-scale door-to-door canvassing operations
		  for state-wide offices is a daunting task. Given the
		  hundreds of thousands of voters to be contacted, reaching
		  every targeted person in the last days leading up to
		  Election Day may prove logistically difficult. Expanding
		  the days on which Get Out the Vote (GOTV) activities take
		  place would make conducting labor intensive canvassing
		  campaigns much easier to administer. However, there has
		  been very little attention paid to the timing of campaign
		  outreach and when it is maximally effective. This paper
		  addresses this lacuna by analyzing a door-to-door voter
		  mobilization experiment that randomly varied the timing of
		  campaign outreach.}
}

@Misc{		  nickerson2006c,
