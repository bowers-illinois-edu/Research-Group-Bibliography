	Issn = {0022-0388},
	Pages = {788--838},
	Title = {{The Social Impact of Social Funds in Jamaica: A `Participatory Econometric' Analysis of Targeting, Collective Action, and Participation in Community-Driven Development}},
	Volume = {41},
