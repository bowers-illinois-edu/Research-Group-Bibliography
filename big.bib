@article{chang2010,
	Abstract = {<i>Objectives</i>. We seek to investigate the determinants
		  of corruption$\backslash$nin authoritarian polities. We
		  hypothesize that corruption in
		  nondemocratic$\backslash$nsettings will be greater where
		  the ruling group is personalistic$\backslash$nrather than a
		  political party or a military clique and that it
		  will$\backslash$nbe greater where rulers expect to remain
		  in power longer. We construct$\backslash$na new
		  operationalization of the selectorate theory advanced by
		  Bueno$\backslash$nde Mesquita et al. <i>Methods</i>. We use
		  cross-sectional statistical$\backslash$nanalysis (OLS) to
		  examine a sample of 40-odd authoritarian
		  regimes$\backslash$nas of 2000. <i>Results</i>. Our results
		  indicate that personalistic$\backslash$nand
		  personalistic-hybrid regimes are more prone to corruption
		  than$\backslash$nsingle-party and military regimes and also
		  that rulers who expect$\backslash$nto remain in power for
		  longer are less corrupt. Corroborating
		  previous$\backslash$nstudies, we document that the
		  availability of natural resources and$\backslash$nhigher
		  levels of institutionalized autocracy are associated
		  with$\backslash$ngreater corruption and that wealthier
		  countries experience less corruption.$\backslash$nOur
		  results are consistent with previous studies, including
		  that$\backslash$nof Bueno de Mesquita et al., but because
		  of our reconstruction of$\backslash$nselectorate theory in
		  terms of real-world regime types, they are$\backslash$nmore
		  easily interpretable. Conclusions. Our study sheds light
		  on$\backslash$nwhy African countries are so notoriously
		  corrupt. The personalistic$\backslash$nauthoritarian
		  regimes that have arisen there in the
		  postcolonial$\backslash$nperiod appear especially prone to
		  corruption, whereas military and$\backslash$nsingle-party
		  dictatorships are less corrupt.},
	Author = {Chang, Eric and Golden, Miriam A.},
	Date-Modified = {2018-06-11 15:13:06 +0000},
	Isbn = {0038-4941},
	Issn = {00384941},
	Journal = {Social Science Quarterly},
	Pages = {1--20},
	Title = {{Sources of corruption in authoritarian regimes}},
	Volume = {91},
	Year = {2010},
	Bdsk-Url-1 = {https://doi.org/10.1111/j.1540-6237.2010.00678.x}}

