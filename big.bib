@unpublished{abrami2010,
	Abstract = {Over the past year, Chinese newspapers have been filled
		  with commentary on changes in the Vietnamese polity, which
		  they deem to be more democratic. The discussion has been so
		  intense that Chinese officials actually banned further
		  discussion of the topic. The discussions pose an intriguing
		  puzzle for comparative politics; as none of the indices of
		  governance currently available are able to adequately
		  account for the variation that Chinese journalists deem to
		  be so important. The inability of these scales to capture
		  within-typology variation is an important lacuna in our
		  understanding of the behavior of authoritarian regimes.
		  Using this puzzle as our starting point, we focus our
		  analytical lens on accountability mechanisms within the two
		  single-party states, finding significant differences in the
		  distribution of powers within the party apparatus, between
		  party and government, and with government institutions. We
		  also document increasing separation on the openness and
		  competitiveness of elections for the Central Committee and
		  national parliaments. Our findings open up important
		  avenues for research on regime durability and democratic
		  transition.},
	Author = {Abrami, RM and Malesky, Edmund and Zheng, Y},
	Booktitle = {Available at SSRN 1669712},
	Date-Modified = {2018-06-11 15:13:06 +0000},
	Pages = {418--490},
	Title = {{Vietnam through Chinese Eyes: Divergent Accountability in Single-Party Regimes}},
	Year = {2010},
	Bdsk-Url-1 = {http://papers.ssrn.com/sol3/papers.cfm?abstract%5C_id=1669712}}

