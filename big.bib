@Article{	  haviland2007,
  author	= {Haviland, Amelia and Nagin, Daniel S and Rosenbaum, Paul
		  R},
  date-modified	= {2018-06-11 15:13:07 +0000},
  isbn		= {1082-989X (Print)$\backslash$r1082-989X (Linking)},
  issn		= {1082-989X},
  journal	= {Psychological methods},
  pages		= {247--267},
  pmid		= {17784793},
  title		= {{Combining propensity score matching and group-based
		  trajectory analysis in an observational study.}},
  volume	= {12},
  year		= {2007},
  abstract	= {In a nonrandomized or observational study, propensity
		  scores may be used to balance observed covariates and
		  trajectory groups may be used to control baseline or
		  pretreatment measures of outcome. The trajectory groups
		  also aid in characterizing classes of subjects for whom no
		  good matches are available and to define substantively
		  interesting groups between which treatment effects may
		  vary. These and related methods are illustrated using data
		  from a Montreal-based study. The effects on subsequent
		  violence of gang joining at age 14 are studied while
		  controlling for measured characteristics of boys prior to
		  age 14. The boys are divided into trajectory groups based
		  on violence from ages 11 to 13. Within trajectory group,
		  joiners are optimally matched to a variable number of
		  controls using propensity scores, Mahalanobis distances,
		  and a combinatorial optimization algorithm. Use of variable
		  ratio matching results in greater efficiency than pair
		  matching and also greater bias reduction than matching at a
		  fixed ratio. The possible impact of failing to adjust for
		  an important but unmeasured covariate is examined using
		  sensitivity analysis.},
  bdsk-url-1	= {https://doi.org/10.1037/1082-989X.12.3.247}
}

