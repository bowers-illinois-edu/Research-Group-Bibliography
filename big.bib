@article{gerber1998,
	Abstract = {Gerber challenges the traditional view that challenger
		  spending is more effective at winning Senate elections than
		  incumbent spending, because when OLS regressions are
		  re-estimated using an instrumental variables such as
		  candidate wealth, he finds that traditional OLS regressions
		  have historically underestimated the effectiveness of
		  incumbent spending. Although most research on campaign
		  spending focuses on challengers rather than incumbents,
		  incumbent spending clearly wins elections, making campaign
		  spending a critical factor in the competitiveness of
		  congressional elections. Spending limits applied to both
		  challengers and incumbents favor those already elected.
		  Therefore limits on campaign spending will only be
		  detrimental to challengers. The author used a reduced form
		  relationship to summarize the mechanism by which spending
		  influences voting, which results in a useful approximation
		  of a complicated process, and that further work is needed
		  in areas such as sources of candidate funds.},
	Author = {Gerber, Alan},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Isbn = {00030554},
	Issn = {00030554},
	Journal = {American Political Science Review},
	Pages = {401--411},
	Title = {{Estimating the effect of campaign spending on senate election outcomes using instrumental variables}},
	Volume = {92},
	Year = {1998},
	Bdsk-Url-1 = {http://www.jstor.org/stable/10.2307/2585672},
	Bdsk-Url-2 = {https://doi.org/10.2307/2585672}}

