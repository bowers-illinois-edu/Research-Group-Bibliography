@article{rubenson2000,
	Abstract = {This paper critically examines two explanations for why
		  people participate in politics: social capital theory and
		  Verba, Schlozman, and Brady's Civic Voluntarism Model. The
		  two approaches are outlined, and it is argued that they in
		  fact share several key characteristics and therefore, also
		  fall victim to some of the same fallacies. These
		  similarities include the focus on non-political
		  institutions as the determents of political activity and
		  the idea of resources or capital being part of the process
		  leading to participation. Both theories neglect the
		  importance of political institutions and state structure in
		  creating opportunities for and influencing the nature of
		  political participation. The example of study circles in
		  Sweden is examined in an attempt to link political
		  participation to political context.},
	Author = {Rubenson, Daniel},
	Date-Modified = {2018-06-11 15:13:08 +0000},
	Journal = {Political Science},
	Title = {{Participation and Politics : Social Capital , Civic Voluntarism , and Institutional Context}},
	Year = {2000},
	Bdsk-Url-1 = {http://www.essex.ac.uk/ECPR/events/jointsessions/paperarchive/copenhagen/ws13/rubenson.PDF}}

@misc{rubenson2004,
	Abstract = {We explore a number of explanations for the sharp
		  difference in voter turnout between the post-generation X
		  cohort and older citizens, using data from the 2000
		  Canadian Election Study. The gap in turnout between these
		  groups is more than 27 percentage points. Controlling for
		  socio-demographic factors reduces the age gap by almost a
		  third. If we control for respondents' perception of the
		  closeness of the race in their riding, whether they were
		  contacted during the campaign and whether they identify
		  with a political party, the age gap decreases by a further
		  three points --- a reduction of 43\% in the original gap.
		  Contrary to conventional wisdom, we find that cynicism and
		  negative attitudes toward politics and politicians are poor
		  explanations for the discrepancy in turnout between young
		  and old. Finally, if we include political information and
		  interest in the model, there is no statistically
		  significant difference in turnout between young and old
		  citizens.Acta Politica (2004) 39, 407--421.
		  doi:10.1057/palgrave.ap.5500079},
	Author = {Rubenson, Daniel and Blais, Andr\'{e} and Fournier, Patrick and Gidengil, Elisabeth and Nevitte, Neil},
	Booktitle = {Acta Politica},
	Date-Modified = {2018-06-11 15:13:08 +0000},
	Issn = {0001-6810},
	Pages = {407--421},
	Title = {{Accounting for the Age Gap in Turnout}},
	Volume = {39},
	Year = {2004},
