@Article{	  marc:1997:usin,
  author	= {Marcus, Sue M.},
  copyright	= {Copyright 1997 American Educational Research Association},
  date-added	= {2014-03-31 11:39:34 +0000},
  date-modified	= {2016-11-02 03:20:24 +0000},
  issn		= {1076-9986},
  journal	= {Journal of Educational and Behavioral Statistics},
  jstor_articletype={Full Length Article},
