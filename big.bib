	Issn = {00179124},
	Keywords = {Cost,Health care research,Outcomes research,Quality of care},
	Pmid = {24588413},
	Title = {{Template Matching for Auditing Hospital Cost and Quality}},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1111/1475-6773.12156}}

