@misc{john2000,
	Abstract = {Part of a special issue on new challenges for urban
		  governance. The writer contends that the creation and
		  development of the European Union (EU), which has legal
		  powers over nation-states, has profound implications for
		  the sub-national politics of the 15 member-states and the
		  new entrants. He asserts that even if the EU has relatively
		  few responsibilities compared with nation-states, it
		  generates a different basis for the exercise of political
		  power and authority. In addition, he argues that the
		  balance between national and supranational power in the EU
		  makes it more difficult to determine the role of
		  sub-national governments. Furthermore, he considers the
		  mechanisms used by sub-national authorities to influence
		  European policies and how the EU changes the relationship
		  between central and local government.},
	Author = {John, Peter},
	Booktitle = {Urban Studies},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Issn = {0042-0980},
	Pages = {877--894},
