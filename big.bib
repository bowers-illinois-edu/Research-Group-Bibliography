@article{ferraz2012,
	Abstract = {This paper examines if money matters in education by
		  looking at whether missing resources due to corruption
		  affect student outcomes. We use data from the auditing of
		  Brazil's local governments to construct objective measures
		  of corruption involving educational block grants
		  transferred from the central government to municipalities.
		  Using variation in the incidence of corruption across
		  municipalities and controlling for student, school, and
		  municipal characteristics, we find a significant negative
		  association between corruption and the school performance
		  of primary school students. Students residing in
		  municipalities where corruption in education was detected
		  score 0.35 standard deviations less on standardized tests,
		  and have significantly higher dropout and failure rates.
		  Using a rich dataset of school infrastructure and teacher
		  and principal questionnaires, we also find that school
		  inputs such as computer labs, teaching supplies, and
		  teacher training are reduced in the presence of corruption.
		  Overall, our findings suggest that in environments where
		  basic schooling resources are lacking, money does matter
		  for student achievement. ?? 2012 Elsevier B.V.},
	Author = {Ferraz, Claudio and Finan, Frederico and Moreira, Diana B.},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Isbn = {0047-2727},
	Issn = {00472727},
	Journal = {Journal of Public Economics},
	Keywords = {Corruption,Development,Education},
	Pages = {712--726},
	Title = {{Corrupting learning. Evidence from missing federal education funds in Brazil}},
	Volume = {96},
	Year = {2012},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.jpubeco.2012.05.012}}

