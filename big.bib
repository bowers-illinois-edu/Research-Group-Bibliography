	Keywords = {Africa,Democracy,Elections,PED11,Participation,Participatory,Week 5},
	Mendeley-Tags = {Africa,Democracy,Elections,PED11,Participation,Participatory,Week 5},
	Pages = {583--622},
	Title = {{The Role of Leaders in Democratic Deliberations: Results from a Field Experiment in S\~{a}o Tom\'{e} and Pr\'{\i}ncipe}},
	Volume = {58},
	Year = {2006},
	Bdsk-Url-1 = {https://doi.org/10.1353/wp.2007.0008}}

