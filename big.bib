@Article{	  corstange2013,
  author	= {Corstange, Daniel},
  date-modified	= {2018-06-11 15:13:07 +0000},
  isbn		= {0007123412000},
  issn		= {1469-2112},
  journal	= {British Journal of Political Science},
  pages		= {1--26},
  title		= {{Ethnicity on the Sleeve and Class in the Heart}},
  volume	= {43},
  year		= {2013},
  abstract	= { ABSTRACT Are voting rights a class or an ethnic issue?
		  They are both$\backslash$nin the diverse societies of the
		  developing world, yet the salience$\backslash$nof ethnicity
		  in public discourse frequently compels people to
		  articulate$\backslash$nidentity demands that may be
		  inconsistent with their material
		  interests,$\backslash$nparticularly among the rich. This
		  article examines these ideas with$\backslash$nan augmented
		  list experiment about illiterate voting rights in
		  Lebanon.$\backslash$nConsistent with received wisdom,
		  direct questions yield identity-based$\backslash$nanswers
		  in which Shiites are more supportive of voting rights
		  than$\backslash$nare Sunnis or Christians. Unobtrusive
		  questions, in contrast, yield$\backslash$nanswers about
		  material deprivation in which poor people are
		  more$\backslash$nsupportive of illiterate voting than are
		  rich people. The divergence$\backslash$nbetween public
		  statements and private preferences helps to
		  reconcile$\backslash$ntheoretical predictions that people
		  respond to material incentives$\backslash$nwith empirical
		  findings that they pursue identity interests. },
  bdsk-url-1	= {http://dx.doi.org/10.1017/S0007123412000592$%5Cbackslash$nhttp://www.journals.cambridge.org/abstract%5C_S0007123412000592},
