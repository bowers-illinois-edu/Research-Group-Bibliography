	Keywords = {lfm},
	Month = {July},
	Number = {3},
	Pages = {359--371},
	Posted-At = {2008-01-18 21:49:17},
	Priority = {2},
	Title = {Should we go "home" to eat?: toward a reflexive politics of localism},
	Url = {http://dx.doi.org/10.1016/j.jrurstud.2005.05.011},
	Volume = {21},
	Year = {2005},
