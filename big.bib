@article{jain2003,
	Abstract = {Informal lenders appear to be thriving even in regions
		  where microfinance institutions (MFIs), such as the Grameen
		  Bank, have established lending programs. We suggest that an
		  explanation for this well-documented observation lies in
		  the ubiquitous use of regularly scheduled repayments by
		  MFIs. Borrowers are typically required to repay their loans
		  in tightly structured installments, beginning soon after
		  loan disbursement. This little- remarked aspect of the
		  repayment schedule is usually explained as inculcating
		  'fiscal discipline' among borrowers. We argue that an
		  alternative rationale for this loan repayment structure
		  lies in the difficulty of monitoring borrowers' actions.
		  The potential for moral hazard leads MFIs to use innovative
		  mechanisms, such as regularly scheduled repayments, which
		  indirectly coopt the better-informed informal lenders.
		  Conversely, this installment repayment structure allows
		  informal lenders to survive. Further, we show that this
		  linkage can not only expand the volume of informal lending,
		  but may also raise the interest rate in the informal
		  sector. ?? 2003 Elsevier B.V. All rights reserved.},
	Author = {Jain, Sanjay and Mansuri, Ghazala},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Issn = {03043878},
	Journal = {Journal of Development Economics},
	Keywords = {Informal lenders,Installment repayments,Microcredit,Microfinance institutions,Moral hazard},
	Pages = {253--279},
	Title = {{A little at a time: The use of regularly scheduled repayments in microfinance programs}},
	Volume = {72},
	Year = {2003},
	Bdsk-Url-1 = {https://doi.org/10.1016/S0304-3878(03)00076-2}}

