@Article{	  casey2010,
  author	= {Casey, Katherine},
  date-modified	= {2018-06-11 15:13:06 +0000},
  journal	= {Working paper},
  title		= {{Crossing Party Lines: The Effects of Information on
		  Redistributive Politics}},
  year		= {2010},
  abstract	= {This paper explores how the quality of information
		  available to voters in{\'a}uences the choices they make in
		  the polling booth and in turn a{\S}ects the strategies of
		  political parties competing for their support. To do so,
		  the paper builds a model of redistributive politics under
		  asymmetric information and then tests the resulting
		  propositions with data from recent elections in Sierra
		  Leone. Using the Lindbeck and Weibull (1987) model as a
		  foundation, I incorporate a new determinant of voting
		  choice{\'o} candidate quality{\'o} which is only
		  imperfectly observed by voters. I show that voters with
		  better information about candidates are more likely to
		  cross ethnic party lines to support a high quality
		  candidate. Furthermore, since information encourages voters
		  to consider characteristics like candidate charisma that
		  are di cult for parties to observe, it makes party
		  forecasting of expected vote shares more uncertain. Such
		  electoral uncertainty in turn induces parties to spread
		  their resources more evenly across jurisdictions. Two
		  institutional attributes of the empirical setting{\'o}
		  ethnicity-based politics and decentralization{\'o} enable
		  direct tests of these informational propositions as well as
		  a novel identi{\"O}cation strategy for the classic swing
		  voter hypothesis. My results suggest that information could
		  break the low accountability equilibrium in which citizens
		  cast their votes blindly along partisan lines, creating
		  little incentive for political parties to invest in
		  candidate quality or provide resources to areas outside the
		  most tightly contested jurisdictions.},
  bdsk-url-1	= {http://cega.berkeley.edu/assets/miscellaneous%5C_files/wgape/19%5C_Casey.pdf}
}

@Article{	  casey2011,
  author	= {Casey, Katherine},
  journal	= {Development},
  title		= {{Reshaping Institutions : Evidence on External Aid and
		  Local Collective Action}},
  year		= {2011},
  abstract	= {Although institutions are believed to be key determinants
		  of economic performance, there is limited evidence on how
		  they can be successfully reformed. The most popular
		  strategy to improve local institutions in developing
		  countries is ``community driven development'' (CDD). This
		  paper estimates the impact of a CDD program in post-war
		  Sierra Leone using a randomized experiment and novel
		  outcome measures. We find positive short-run effects on
		  local public goods provision, but no sustained impacts on
