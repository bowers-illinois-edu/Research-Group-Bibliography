	Bdsk-Url-1 = {https://doi.org/10.1080/00220380500145297}}

@article{rao2008,
	Abstract = {Most economists think of common property as physical -- a
		  body of water, a forest -- and as $\backslash$nbounded
		  within geographic space. In this paper, building on work in
		  social theory, I argue that $\backslash$ncommon property
		  can also be social -- defined within symbolic space. People
		  can be bound by $\backslash$nwell-defined symbolic
		  agglomerations that have characteristics similar to common
		  property. I $\backslash$ncall these ``symbolic public
		  goods'' (SPGs) and make the case that such constructs are
		  central to $\backslash$nunderstanding collective action.
		  The point is illustrated by contrasting how conceptions of
		  $\backslash$nnationalism in Indonesia and India created
		  SPGs that resulted in very different strategies of local
		  $\backslash$ndevelopment. Indonesia emphasized collective
		  action by the poor that resulted in a form of
		  $\backslash$nregressive taxation, enforced by the ideology
		  of svadaya gotong royong (community self-help)
		  $\backslash$nthat was both internalized and coercively
		  enforced. India emphasized democratic decentralization
		  $\backslash$nvia the panchayat system driven by the
		  Gandhian ideology of gram swaraj (self-reliant villages).
		  $\backslash$nThis has resulted in an unusual
		  equity-efficiency tradeoff; Indonesia has delivered public
		  services $\backslash$nmore efficiently than India, but at
		  the cost of democratic freedoms and voice. I argue that the
		  $\backslash$nchallenge for these countries is to not
		  undermine their existing SPGs but to build on them;
		  $\backslash$nIndonesia should retain the spirit of svadaya
		  gotong royong but to channel it in an equitable and
		  $\backslash$ndemocratic direction, while India should build
		  the capacity of the panchayat system by giving it
		  $\backslash$nfiscal teeth, while promoting underutilized
		  institutions such as Gram Sabhas that encourage
		  $\backslash$naccountability and transparency.},
	Author = {Rao, Vijayendra},
	Journal = {The Contested Commons: Conversations Between Economists and Anthropologists},
	Pages = {1--26},
	Title = {{Symbolic Public Goods and the Coordination of Collective Action: A Comparison of Local Development in India and Indonesia}},
	Year = {2008}}

