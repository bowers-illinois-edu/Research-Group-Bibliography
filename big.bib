@Article{	  gottlieb2010,
  author	= {Gottlieb, Jessica},
  date-modified	= {2018-06-11 15:13:07 +0000},
  file		= {::},
  journal	= {Working Group on African Political Economy, \ldots},
  pages		= {1--34},
  title		= {{Is democracy working ? Determinants of local government
		  performance (failure) in Mali}},
  year		= {2010},
  abstract	= {This paper contributes to the ongoing debate about
		  democracy's role in development by identifying avenues of
		  democratic failure in a developing country context. I argue
		  that particular featrues of developing countries make
		  voters less likely to control politicians and political
		  parties more likely to collude with one another. Using a
		  unique local-level dataset from one developing democracy,
		  Mali, I test these sources of democratic failure and the
		  conditions that make them more or less likely. I explore
		  variation in local public goods provision across localities
		  as a proxy for understanding the determinants of government
		  performance. I nd that voters are better able to control
		  politicians when parties are more numer- ous and fragmented
		  and when information asymmetries are reduced, but that the
		  eects of party competition are conditional on information.
		  I also nd that collusion is less likely in the prescence of
		  relatively strong opposition parties that provide a
		  credible threat. Matching and regression discontinuity are
		  some of the identication strategies used to reach these
		  conclusions.},
  bdsk-url-1	= {http://cega.berkeley.edu/assets/miscellaneous%5C_files/wgape/18%5C_Gottlieb.pdf}
}

