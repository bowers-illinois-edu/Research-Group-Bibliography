	Title = {{The Europeanisation of Sub-national Governance}},
	Volume = {37},
	Year = {2000},
	Bdsk-Url-1 = {https://doi.org/10.1080/00420980050011136}}

@article{john2003,
	Abstract = {This article reviews the current state of public policy
		  theory to find out if researchers are ready to readdress
		  the research agenda set by the classic works of Baumgartner
		  and Jones (1993), Kingdon (1984) and Sabatier and
		  Jenkins-Smith (1993). After reviewing the influ- ences of
		  institutional, rational choice, network, socio-economic and
		  ideational approaches, the article pays tribute to the
		  policy streams, punctuated equilibrium and policy advocacy
		  coalition frameworks whilst also suggesting that future
		  theory and research could identify more precisely the
		  causal mechanisms driving policy change. The article argues
		  that evo- lutionary theory may usefully uncover the
		  micro-level processes at work, particularly as some the
		  three frameworks refer to dymamic models and methods. After
		  reviewing some evolutionary game theory and the study of
		  memes, the article suggests that the benefits of
		  evolutionary theory in extending policy theories need to be
		  balanced by its limitations},
	Author = {John, Peter},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Isbn = {1541-0072},
	Issn = {0190292X},
	Journal = {Policy Studies Journal},
	Pages = {481--498},
	Title = {{Is there life after policy streams, advocacy coalitions, and punctuations: Using evolutionary theory to explain policy change?}},
	Volume = {31},
	Year = {2003},
	Bdsk-Url-1 = {https://doi.org/10.1111/1541-0072.00039}}

