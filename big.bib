@article{hollyer2011,
	Abstract = {Corruption is typically depicted as a result of one of two
		  factors: a lack of politial$\backslash$naccountability or
		  insufficient state capacity. Nonetheless, substantial
		  variation in$\backslash$ncorruption levels exists even
		  within the set of politically unaccountable
		  high-capacity$\backslash$nregimes. In this paper, we
		  examine a third determinant of corruption -- the
		  ideological$\backslash$nappeal of the government -- and
		  demonstrate that this variable can explain
		  variation$\backslash$nin the types and levels of corruption
		  experienced in politically unaccountable
		  regimes.$\backslash$nUsing a model of both moral hazard and
		  adverse selection, we predict that (1)
		  regimes$\backslash$nthat inspire the intense ideological
		  loyalty of the populace are likely to enjoy low
		  levels$\backslash$nof petty corruption and that (2)
		  autocratic regimes that enjoy such intense
		  support$\backslash$nfrom only a narrow segment of the
		  populace will erect credible anti-corruption
		  institutions.$\backslash$nPolitical corruption, by
		  contrast, need not covary with levels of
		  ideological$\backslash$nsupport. We illustrate the
		  mechanisms of our model through a series of case
		  studies$\backslash$nthat demonstrate the importance of
		  ideology in driving levels of corruption -- with
		  a$\backslash$nparticular focus on low levels of corruption
		  in `developmentalist' regimes. Finally,
		  we$\backslash$ndiscuss the relevance of our findings to
		  other -- democratic -- political settings.},
	Author = {Hollyer, James R and Wantchekon, Leonard},
	Date-Modified = {2018-06-11 15:13:07 +0000},
	Issn = {1556-5068},
	Journal = {New York},
	Pages = {1--44},
	Title = {{Corruption in Autocracies ∗}},
	Year = {2011},
	Bdsk-Url-1 = {https://doi.org/10.2139/ssrn.1861464}}

